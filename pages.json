{	"easycom": {
		"^u-(.*)": "@/lib/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页,
		{
			"path": "pages/login/login",
			"style": {}
		}, {
			"path": "pages/login/forget",
			"style": {}
		}, {
			"path": "pages/login/register",
			"style": {}
		}

		,
		//大布局页
		{
			"path": "pages/index/index",
			"style": {
				"disableScroll": true, // 禁止原生页面滚动, 解决Android小程序下拉卡顿的问题
				"mp-alipay":{"allowsBounceVertical":"NO"} // 取消支付宝和钉钉小程序的iOS回弹,避免与下拉刷新冲突 (可统一配在 'globalStyle')
			}
		}
	    ,{
            "path" : "pages/startcheck/startcheck",
            "style" : {}
        }
        ,{
            "path" : "pages/evaluate/evaluate",
            "style" : {}
        }
        ,{
            "path" : "pages/evaluate/befored/befored",
            "style" : {}
        }
        ,{
            "path" : "pages/evaluate/working/working",
            "style" : {}
        }
        ,{
            "path" : "pages/evaluate/safe/safe",
            "style" : {}
        }
        ,{
            "path" : "pages/evaluate/summary/summary",
            "style" : {}
        }
        ,{
            "path" : "pages/improvement/improvement",
            "style" : {}
        }
        ,{
            "path" : "pages/ring/ring",
            "style" : {}
        }
        ,{
            "path" : "pages/readpackagefeedback/readpackagefeedback",
            "style" : {}
        }
        ,{
            "path" : "pages/readpackagefeedback/feedbackdetail/feedbackdetail",
            "style" : {}
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "大修质量",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"navigationStyle": "custom",
		"backgroundColorTop":"#FFFFFF",
		"app-plus" : {
			"bounce" : "none" // 取消APP端iOS回弹,避免与下拉刷新冲突 (可统一配在 'globalStyle')
		}
	}
}
